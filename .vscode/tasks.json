{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe build active file",
            "command": "C:\\msys64\\mingw64\\bin\\g++.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-E",
                "${file}",
                ">",
                "${fileBasenameNoExtension}.txt",
                "&",
                "g++",
                "-g",
                "${file}",
                "-o",
                "${workspaceRoot}\\Output\\${fileBasenameNoExtension}.exe"
            ],

            "options": {
                "cwd": "${fileDirname}"
            },

            "problemMatcher": [
                "$gcc"
            ],

            "group": {
                "kind": "build",
                "isDefault": true
            },

            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}

